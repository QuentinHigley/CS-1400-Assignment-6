<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" prefHeight="430.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <HBox alignment="CENTER" prefHeight="353.0" prefWidth="590.0" GridPane.columnIndex="1">
         <children>
            <VBox prefHeight="353.0" prefWidth="308.0">
               <children>
                  <HBox prefHeight="207.0" prefWidth="198.0">
                     <children>
                        <Slider fx:id="slrValue" blockIncrement="1.0" majorTickUnit="1.0" max="1000" maxHeight="200.0" maxWidth="14.0" min="100" minHeight="50.0" minWidth="14.0" minorTickCount="1" orientation="VERTICAL" snapToTicks="true" value="100.0">
                           <HBox.margin>
                              <Insets left="10.0" right="10.0" />
                           </HBox.margin>
                        </Slider>
                        <Label fx:id="lblSlrValue" text="${slrValue.value}" />
                     </children>
                  </HBox>
                  <Button fx:id="btnRoll" alignment="CENTER" mnemonicParsing="false" onAction="#Roll" prefHeight="29.0" prefWidth="247.0" text="Execute">
                     <VBox.margin>
                        <Insets left="10.0" top="10.0" />
                     </VBox.margin>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                  </Button>
               </children>
            </VBox>
         </children>
      </HBox>
      <TextArea fx:id="txtOutput" prefHeight="353.0" prefWidth="663.0">
         <GridPane.margin>
            <Insets left="20.0" />
         </GridPane.margin>
      </TextArea>
   </children>
</GridPane>
